# OpenAI API key (required)
OPENAI_API_KEY=YOUR_OPENAI_KEY

# When true, the bot will print group IDs for easy setup
SETUP_MODE=true

# Comma-separated list of WhatsApp group IDs allowed to use the bot
# Example: 12345-67890@g.us,98765-43210@g.us
ALLOWED_GROUPS=000123456789-0123456789@g.us,000123456789-0123456789@g.us
